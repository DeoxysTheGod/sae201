     } else {\r\n                    data.add(row);\r\n                }\r\n            }\r\n\r\n            // Set the data items of the TableView\r\n            tableView.setItems(data);\r\n        } catch (IOException e) {\r\n            e.printStackTrace();\r\n        }\r\n\r\n        // Create the layout and scene\r\n        VBox vbox = new VBox(tableView);\r\n        Scene scene = new Scene(vbox, 800, 600);\r\n        primaryStage.setScene(scene);\r\n        primaryStage.show();\r\n    }\r\n\r\n}
